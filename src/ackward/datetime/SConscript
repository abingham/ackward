import build_utils

Import(['env'])

env.Akw('DateBase.akw')

sources = [
    'Date.cpp',
    'DateTime.cpp',
    'Initialize.cpp',
    'Module.cpp',
    'TimeDelta.cpp',
    'TZInfo.cpp',
    ]

build_utils.build_shared_library(env,
                                 'datetime',
                                 sources,
                                 deps=['time'])
