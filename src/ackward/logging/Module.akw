# -*- python -*-

import ackward as akw

def definition():
    m = akw.Module(name="logging")

    akw.Function(
        mod=m,
        name="getLogger",
        return_type="Logger")

    akw.Function(
        mod=m,
        name='getLogger',
        signature=[('std::wstring', 'name')],
        return_type='Logger')

    return m
