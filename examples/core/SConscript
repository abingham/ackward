import build

Import(['env'])

programs = {
    'bytearray_example' : ['ByteArray.cpp'],
    'bytes_example' : ['Bytes.cpp'],
    'exception_translation_example' : ['ExceptionTranslation.cpp'],
    'exception_translator_example' : ['ExceptionTranslator.cpp'],
    }

for name, sources in programs.items():
    build.program(
        env,
        name,
        sources,
        deps=['core'])
